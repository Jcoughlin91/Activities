const pizzaToppings = ["Cheese", "meat", "veggies", "Pineapple"];
function greetCustomer() {
console.log("Welcome to the Pizza Club! The toppings are") }
greetCustomer();
for (let toppings of pizzaToppings) {
console.log(toppings)
}

//3
function getPizzaOrder(size, crust, ...toppings){
console.log(`One ${size} pizza with ${crust} and ${toppings}`);  
} 
getPizzaOrder("medium", "thick","Cheese", "meat", "veggies", "Pineapple");
return getPizzaOrder


//4
function preparePizza (medium, thick, ...toppings) {
  let Pizza = {
    size: medium [0]
    crust: thick [1]
    toppings: ...toppings [2]
  return (size, crust, ...toppings)
};

return console.log(Pizza);
}
